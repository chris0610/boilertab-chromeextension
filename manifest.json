{
    "name": "BoilerTab",
    "version": "1.0.0.2",
    "manifest_version": 2,
    "description": "Stay on track of things or jot down flitting ideas.",
    "icons": { 
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png" 
    },
    "chrome_url_overrides": {
      "newtab": "overrides/override.html"
    },
    "permissions": [
        "storage"
    ]
  }